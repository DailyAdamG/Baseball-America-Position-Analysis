FilteredData <- filter(PitchersAndPositionPlayers, TotalWAR > 0)
View(FilteredData)
ordered_position <- factor(FilteredData$Position, levels = c("Pitcher", "Position Player"))
ggplot(data = FilteredData, aes(x = TotalWAR, fill = ordered_position)) +
geom_density(alpha = .2) +
scale_fill_discrete(labels = c("Pitchers", "Position Players")) +
scale_x_continuous(breaks = seq(0, 60, 5)) +
labs(title = "Density Plot of Team Controlled WAR", x = "WAR", y = "Density", fill = "Position") +
theme(plot.title = element_text(hjust = 0.5), legend.title = element_text(hjust = 0.5))
